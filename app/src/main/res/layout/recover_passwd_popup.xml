<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/frame_157"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"

        android:background="@drawable/frame_157"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/amico" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="182dp"
            android:layout_height="38dp"
            android:layout_below="@+id/imageView2"
            android:layout_marginTop="20dp"
            android:textSize="16sp"
            android:textAlignment="center"
            android:textStyle="bold"
            android:layout_centerHorizontal="true"
            android:text="@string/a_reset_lin"
            android:textAppearance="@style/a_reset_lin" />

        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/frame_155"
            android:layout_width="44dp"
            android:layout_height="39dp"
            android:background="@drawable/frame_155"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textView3"
            android:layout_marginTop="20dp"
            >

            <TextView
                android:id="@+id/ok"
                android:layout_width="24dp"
                android:layout_height="19dp"
                android:text="@string/ok"
                android:textAppearance="@style/ok"
                android:gravity="top|center_horizontal"

                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                />


        </RelativeLayout>




    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>